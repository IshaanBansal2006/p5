{
  "name": "@p5/cli",
  "version": "0.1.0",
  "description": "P5: hackathon devflow in a box (testing, README sync, Devpost, Playwright wizard).",
  "type": "module",
  "bin": {
    "p5": "bin/p5.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin",
    "templates"
  ],
  "scripts": {
    "build": "tsc && cp -r src/templates dist/",
    "dev": "tsc -w",
    "prepublishOnly": "npm run build",
    "prepare": "husky",
    "p5:test": "p5 test",
    "p5:readme": "p5 readme sync",
    "p5:devpost": "p5 devpost gen",
    "p5:init": "p5 init"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^21.0.0",
    "@playwright/test": "^1.55.1",
    "chalk": "^5.3.0",
    "execa": "^9.3.0",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.2",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "semver": "^7.6.3",
    "yaml": "^2.5.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.0",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "esbuild": "^0.23.0",
    "eslint": "^9.0.0",
    "husky": "^9.1.7",
    "puppeteer-core": "^24.22.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}
